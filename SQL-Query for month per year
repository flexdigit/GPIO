select tstamp,
case cast (strftime('%m', tstamp) as integer)
when 1 then 'Jan'
when 2 then 'Feb'
when 3 then 'Mar'
when 4 then 'Apr'
when 5 then 'May'
when 6 then 'Jun'
when 7 then 'Jul'
when 8 then 'Aug'
when 9 then 'Sep'
when 10 then 'Oct'
when 11 then 'Nov'
when 12 then 'Dez'
else 'fehler' end,
sum(tick) from gascounter
GROUP BY strftime('%m', tstamp)
